<Test>
  <Inputs>

    <!-- Equal 64-bits -->
    <ValueSet InputId="0">
      <Register Name="T0" Value0="0xFFFFFFFE" Value1="0x00000001" />
			<Register Name="T1" Value0="0xFFFFFFFE" Value1="0x00000001" />
		</ValueSet>

		<!-- Not Equal 64-bits -->
		<ValueSet InputId="1">
			<Register Name="T0" Value0="0xFFFFFFFE" Value1="0x00000001" />
			<Register Name="T1" Value0="0xFFFFFFFF" Value1="0x00000001" />
		</ValueSet>

	</Inputs>

  <Instances>

    <Instance Id="0">
      BNE T0, T1, 8
    </Instance>

  </Instances>

  <Outputs>

    <ValueSet InputId="0" InstanceId="0">
			<SpecialRegister Name="DelaySlot" Value0="0" />
    </ValueSet>

		<ValueSet InputId="1" InstanceId="0">
			<SpecialRegister Name="DelaySlot" Value0="1" />
		</ValueSet>

	</Outputs>

</Test>
